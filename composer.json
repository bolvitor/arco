    "require": {
        "mpdf/mpdf": "*",
        "phpmailer/phpmailer": "^6.6",
        "dompdf/dompdf": "^2.0",
        "vlucas/phpdotenv": "^5.5",
        "phpoffice/phpexcel": "^1.8",
        "phpoffice/phpspreadsheet": "^1.12"

    }
}
